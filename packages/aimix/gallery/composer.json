{
  "name": "aimix/gallery",
  "description": "A gallery",
  "license": "MIT",
  "keywords": [
    "gallery"
  ],
  "type": "library",
  "authors": [
    {
      "name": "",
      "email": ""
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^7.3"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Aimix\\Gallery\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aimix\\Gallery\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aimix\\Gallery\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
